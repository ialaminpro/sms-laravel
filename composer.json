{
    "name": "acolyte/sms-laravel",
    "description": "SMS - A simple Laravel package.",
    "homepage": "https://github.com/ialaminpro/sms-laravel",
    "keywords": [
		"sms",
		"sms api",
		"alamin",
		"laravel sms",
		"message"
	],
    "license": "MIT",
    "authors": [
        {
            "name": "Al Amin",
            "email": "ialamin.pro@gmail.com",
            "homepage": "https://github.com/ialaminpro",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-0": {
            "Acolyte\\SMS\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Acolyte\\SMS\\SMSServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ialaminpro/sms-laravel"
        }
    ]
}
